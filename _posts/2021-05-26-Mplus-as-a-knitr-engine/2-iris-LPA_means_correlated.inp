TITLE: iris LPA

DATA:
    File is iris.dat
    
VARIABLE: 

    Names are x1, x2, x3, x4;

    Classes = c(2) ;
            
MODEL:
    
    %overall%
    
    x1 x2 x3 x4; 
    
    x1 WITH x2-x4;
    x2 WITH x3-x4;
    x3 WITH x4;

    %c#1%
    
    [x1-x4];
    
    %c#2%
    
    [x1-x4];
    
ANALYSIS: 
    Type is mixture;
            
OUTPUT:
    Tech11;